{
  "name": "ios-development",
  "version": "1.1.0",
  "owner": {
    "name": "iOS Development",
    "email": "carolanelefebvre@okorp.fr"
  },
  "description": "Ultra-granular iOS development agents (1 plugin = 1 job). Swift 6.2, SwiftUI, SwiftData, and complete Apple ecosystem support.",
  "homepage": "https://github.com/CarolaneLFBV/ios-development-agents",
  "repository": "https://github.com/CarolaneLFBV/ios-development-agents",
  "pluginRoot": "./plugins",
  "plugins": [
    {
      "name": "ios-architecture",
      "source": "./plugins/ios-architecture",
      "description": "iOS architecture patterns specialist (MVVM, TCA, Clean Architecture ONLY)",
      "version": "1.0.0",
      "category": "architecture",
      "keywords": ["architecture", "mvvm", "tca", "clean", "patterns"],
      "author": {
        "name": "iOS Development"
      },
      "license": "MIT",
      "strict": false
    },
    {
      "name": "swift-language",
      "source": "./plugins/swift-language",
      "description": "Swift 6.2 language features specialist (generics, protocols, property wrappers, InlineArray, integer generic parameters ONLY)",
      "version": "1.0.0",
      "category": "core",
      "keywords": ["swift", "generics", "protocols", "language", "swift62"],
      "author": {
        "name": "iOS Development"
      },
      "license": "MIT",
      "strict": false
    },
    {
      "name": "swift-concurrency",
      "source": "./plugins/swift-concurrency",
      "description": "Swift 6.2 Concurrency specialist (async/await, actors, @MainActor, nonisolated async, Observations ONLY)",
      "version": "1.0.0",
      "category": "core",
      "keywords": ["concurrency", "async", "await", "actors", "swift62"],
      "author": {
        "name": "iOS Development"
      },
      "license": "MIT",
      "strict": false
    },
    {
      "name": "swiftui-views",
      "source": "./plugins/swiftui-views",
      "description": "SwiftUI view composition specialist (layout, custom views, ViewBuilder ONLY)",
      "version": "1.0.0",
      "category": "ui",
      "keywords": ["swiftui", "views", "layout", "composition"],
      "author": {
        "name": "iOS Development"
      },
      "license": "MIT",
      "strict": false
    },
    {
      "name": "swiftui-state",
      "source": "./plugins/swiftui-state",
      "description": "SwiftUI state management specialist (@State, @Observable, @Binding ONLY)",
      "version": "1.0.0",
      "category": "ui",
      "keywords": ["swiftui", "state", "observable", "binding"],
      "author": {
        "name": "iOS Development"
      },
      "license": "MIT",
      "strict": false
    },
    {
      "name": "swiftdata-models",
      "source": "./plugins/swiftdata-models",
      "description": "SwiftData model architect (@Model, relationships, attributes ONLY)",
      "version": "1.0.0",
      "category": "data",
      "keywords": ["swiftdata", "models", "schema", "relationships"],
      "author": {
        "name": "iOS Development"
      },
      "license": "MIT",
      "strict": false
    },
    {
      "name": "ios-orchestration",
      "source": "./plugins/ios-orchestration",
      "description": "Multi-agent workflow orchestration (complex feature workflows ONLY)",
      "version": "1.0.0",
      "category": "orchestration",
      "keywords": ["orchestration", "workflow", "multi-agent"],
      "author": {
        "name": "iOS Development"
      },
      "license": "MIT",
      "strict": false
    }
